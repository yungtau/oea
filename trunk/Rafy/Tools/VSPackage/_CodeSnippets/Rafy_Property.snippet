<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Description>一般属性</Description>
            <Title>Rafy Property</Title>
            <Shortcut>RafyProperty</Shortcut>
            <Author>HuQingfang</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
        </Header>
        <Snippet>
            <Declarations>
                <Literal>
                    <ID>PropertyType</ID>
                    <ToolTip>Property type</ToolTip>
                    <Default>string</Default>
                </Literal>
                <Literal>
                    <ID>PropertyName</ID>
                    <Default>Name</Default>
                </Literal>
                <Literal Editable="false">
                    <ID>ClassName</ID>
                    <Function>ClassName()</Function>
                    <Default>ClassNamePlaceholder</Default>
                </Literal>
            </Declarations>
            <Code Language="csharp">
                <![CDATA[public static readonly Property<$PropertyType$> $PropertyName$Property = P<$ClassName$>.Register(e => e.$PropertyName$);
        /// <summary>
        /// $end$
        /// </summary>
        public $PropertyType$ $PropertyName$
        {
            get { return this.GetProperty($PropertyName$Property); }
            set { this.SetProperty($PropertyName$Property, value); }
        }]]>
            </Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>