@{
    ViewBag.Title = "实体模块";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@section head{
    <script type="text/javascript">
        Ext.onReady(function () {
            Ext.QuickTips.init();
            Oea.App.showModel({
                model: '@this.ViewBag.Type',
                viewName: '@this.ViewBag.ViewName',
                isAggt: @(this.ViewBag.IsAggt ? "true" : "false")
            });
        });
    </script>
}
