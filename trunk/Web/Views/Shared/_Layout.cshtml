<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/ExtJs/resources/css/ext-all.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    @if (Context.IsDebuggingEnabled)
    {
    <script src="@Url.Content("~/ExtJs/ext-all-debug.js")" type="text/javascript"></script>
    }
    else
    {
    <script src="@Url.Content("~/ExtJs/ext-all.js")" type="text/javascript"></script>
    }
    <script src="@Url.Content("~/ExtJs/ext-lang-zh_CN.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/OEA.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/OEAEntity.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/OEACommands.js")" type="text/javascript"></script>
    @RenderSection("head")
</head>
<body>
    @RenderBody()
</body>
</html>
