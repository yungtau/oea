        <sources>
            <source name="System.ServiceModel.MessageLogging">
                <listeners>
                    <add name="xml" type="System.Diagnostics.XmlWriterTraceListener" initializeData="D:\UserTraces.svclog"/>
                </listeners>
            </source>
        </sources>
        <trace autoflush="true"/>
    </system.diagnostics>-->
    <!--
